<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.ezt.video.instasaver.screen.home.fragment.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_storyFragment"
            app:destination="@id/storyFragment"
            />
        <action
            android:id="@+id/action_home_to_DPViewerFragment"
            app:destination="@id/DPViewerFragment" />

        <action
            android:id="@+id/action_home_to_ProfileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/downloads"
        android:name="com.ezt.video.instasaver.screen.home.download.DownloadsFragment"
        android:label="downloads_fragment"
        tools:layout="@layout/fragment_downloads" />
    <fragment
        android:id="@+id/webView"
        android:name="com.ezt.video.instasaver.screen.home.fragment.WebViewFragment"
        android:label="web_view_fragment"
        tools:layout="@layout/fragment_web_view" />
    <fragment
        android:id="@+id/storyFragment"
        android:name="com.ezt.video.instasaver.screen.home.story.StoryFragment"
        android:label="StoryFragment" >
        <argument
            android:name="cookie"
            app:argType="string"
            />
    </fragment>
    <fragment
        android:id="@+id/DPViewerFragment"
        android:name="com.ezt.video.instasaver.screen.home.dpviewer.DPViewerFragment"
        android:label="dp_viewer_fragment"
        tools:layout="@layout/fragment_dp_viewer" >
        <argument
            android:name="cookie"
            app:argType="string"
            />
    </fragment>

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.ezt.video.instasaver.screen.home.dpviewer.DPViewerFragment"
        android:label="dp_viewer_fragment"
        tools:layout="@layout/fragment_dp_viewer" >
        <argument
            android:name="cookie"
            app:argType="string"
            />
    </fragment>
</navigation>